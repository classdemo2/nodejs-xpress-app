pipeline {
    agent any
  
    stages {
        stage('Wolrd') {
            steps {
                echo 'Hello World'
            }
        }
        stage('Africa') {
            steps {
                echo 'Welcome to Africa!'
            }
        }
        stage('WestAfrica') {
            steps {
                echo 'Welcome to West Africa!'
            }
        }
        stage('Nigeria') {
            steps {
                echo 'Welcome to Nigeria!'
            }
    }
}
}
